<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <!-- Remote Repositories -->
    <remote name="git" fetch="https://github.com" />
    <remote name="glab" fetch="https://gitlab.com" />

    <!-- Removed Projects -->
    <remove-project path="packages/resources/devicesettings" />

    <!-- Device Trees -->
    <project path="device/xiaomi/alioth"
             name="alioth-stuffs/device_xiaomi_alioth"
             remote="git"
             revision="evo"
             clone-depth="1" />

    <project path="device/xiaomi/sm8250-common"
             name="alioth-stuffs/device_xiaomi_sm8250-common"
             remote="git"
             revision="bak"
             clone-depth="1" />

    <!-- Vendor Files -->
    <project path="vendor/xiaomi/alioth"
             name="PocoF3Releases/vendor_xiaomi_alioth"
             remote="git"
             revision="aosp-14-old"
             clone-depth="1" />

    <project path="vendor/xiaomi/sm8250-common"
             name="PocoF3Releases/vendor_xiaomi_sm8250-common"
             remote="git"
             revision="aosp-14-old"
             clone-depth="1" />

    <!-- Kernel -->
    <project path="kernel/xiaomi/sm8250"
             name="PocoF3Releases/kernel_xiaomi_sm8250"
             remote="git"
             revision="aosp-14"
             clone-depth="1" />

    <!-- Hardware -->
    <project path="hardware/xiaomi"
             name="alioth-stuffs/hardware_xiaomi"
             remote="git"
             revision="udc" />

    <!-- Packages -->
    <project path="packages/resources/devicesettings"
             name="PocoF3Releases/packages_resources_devicesettings"
             remote="git"
             revision="aosp-14" />

    <!-- Vendor Keys -->
    <project path="vendor/lineage-priv/keys"
             name="los-alioth/keys"
             remote="git"
             revision="los"
             clone-depth="1" />

    <project path="vendor/xiaomi/camera" name="NotZeetaa/vendor_xiaomi_camera" remote="glab" revision="aosp-14" clone-depth="1" />
</manifest>
